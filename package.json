{
	"name": "octochangelog-webapp",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"pnpm": "^10.26.0"
	},
	"type": "module",
	"packageManager": "pnpm@10.26.2",
	"scripts": {
		"start": "next start",
		"dev": "next dev --webpack",
		"build": "next build --webpack",
		"type-gen": "next typegen",
		"type-check": "pnpm type-gen && tsc --noEmit",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "pnpm lint --fix",
		"prettier-base": "prettier . --ignore-unknown --cache --log-level warn",
		"format": "pnpm prettier-base --write",
		"format:check": "pnpm prettier-base --check",
		"happo": "happo",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:ci": "pnpm test --coverage",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"e2e:report": "playwright show-report",
		"gen:theme-typings": "chakra-cli tokens src/custom-theme.ts --strict-component-types --strict-token-types",
		"prepare": "husky",
		"postinstall": "pnpm gen:theme-typings"
	},
	"dependencies": {
		"@chakra-ui/react": "2.8.2",
		"@emotion/react": "11.14.0",
		"@emotion/styled": "11.14.1",
		"@fontsource-variable/inter": "5.2.8",
		"@fontsource-variable/roboto-mono": "5.2.8",
		"@octokit/auth-callback": "6.0.0",
		"@octokit/rest": "22.0.1",
		"@sentry/nextjs": "10.32.1",
		"@tanstack/react-query": "5.90.16",
		"@unpic/react": "1.0.2",
		"downshift": "9.0.13",
		"es-toolkit": "1.43.0",
		"framer-motion": "12.24.7",
		"js-cookie": "3.0.5",
		"next": "16.1.1",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-icons": "5.5.0",
		"rehype-highlight": "7.0.2",
		"rehype-react": "8.0.0",
		"remark-emoji": "5.0.2",
		"remark-gfm": "4.0.1",
		"remark-github": "12.0.0",
		"remark-parse": "11.0.0",
		"remark-rehype": "11.1.2",
		"remark-stringify": "11.0.0",
		"semver": "7.7.3",
		"unified": "11.0.5"
	},
	"devDependencies": {
		"@chakra-ui/cli": "2.4.1",
		"@chakra-ui/styled-system": "2.9.2",
		"@chakra-ui/theme-tools": "2.1.2",
		"@chakra-ui/utils": "2.0.14",
		"@currents/playwright": "1.20.2",
		"@eslint-react/eslint-plugin": "2.6.0",
		"@eslint/compat": "2.0.1",
		"@eslint/js": "9.39.2",
		"@msw/playwright": "0.4.5",
		"@octokit/openapi-types": "25.1.0",
		"@playwright/test": "1.57.0",
		"@stylistic/eslint-plugin": "5.7.0",
		"@tanstack/eslint-plugin-query": "5.91.2",
		"@tanstack/react-query-devtools": "5.91.2",
		"@testing-library/react": "16.3.1",
		"@types/js-cookie": "3.0.6",
		"@types/mdast": "4.0.4",
		"@types/node": "22.19.6",
		"@types/react": "19.2.8",
		"@types/react-dom": "19.2.3",
		"@types/semver": "7.7.1",
		"@types/unist": "3.0.3",
		"@vitejs/plugin-react-swc": "4.2.2",
		"@vitest/coverage-v8": "4.0.17",
		"@vitest/eslint-plugin": "1.6.6",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-playwright": "2.5.0",
		"eslint-plugin-react-hooks": "7.0.1",
		"eslint-plugin-unicorn": "62.0.0",
		"globals": "17.0.0",
		"happo": "6.2.3",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"msw": "2.12.7",
		"prettier": "3.7.4",
		"type-fest": "5.3.1",
		"typescript": "5.9.3",
		"typescript-eslint": "8.53.0",
		"vite-tsconfig-paths": "6.0.4",
		"vitest": "4.0.17"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all",
			"not ie 11"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"msw": {
		"workerDirectory": "public"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.2.8",
			"@types/react-dom": "19.2.3"
		}
	}
}
