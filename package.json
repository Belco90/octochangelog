{
	"name": "octochangelog-webapp",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"pnpm": "^10.26.0"
	},
	"type": "module",
	"packageManager": "pnpm@10.26.2",
	"scripts": {
		"dev": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev",
		"dev:mock": "VITE_API_MOCKING=enabled NODE_OPTIONS='--import ./instrument.server.mjs' vite dev",
		"build": "vite build",
		"postbuild": "cp instrument.server.mjs dist/server/instrument.server.mjs",
		"preview": "vite preview",
		"start": "node --import dist/server/instrument.server.mjs dist/server/server.js",
		"type-check": "tsc --noEmit",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "pnpm lint --fix",
		"prettier-base": "prettier . --ignore-unknown --cache --log-level warn",
		"format": "pnpm prettier-base --write",
		"format:check": "pnpm prettier-base --check",
		"happo": "happo",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:ci": "pnpm test --coverage",
		"test:unit": "vitest --project unit",
		"test:browser": "vitest --project browser",
		"test:browser:headless": "vitest --project browser --browser.headless",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"e2e:report": "playwright show-report",
		"gen:theme-typings": "chakra-cli tokens src/custom-theme.ts --strict-component-types --strict-token-types",
		"prepare": "husky",
		"postinstall": "pnpm gen:theme-typings"
	},
	"dependencies": {
		"@chakra-ui/react": "2.10.9",
		"@emotion/react": "11.14.0",
		"@emotion/styled": "11.14.1",
		"@octokit/auth-callback": "6.0.0",
		"@octokit/rest": "22.0.1",
		"@sentry/tanstackstart-react": "10.34.0",
		"@tanstack/react-query": "5.90.19",
		"@tanstack/react-router": "1.154.7",
		"@tanstack/react-router-ssr-query": "1.154.7",
		"@tanstack/react-start": "1.154.7",
		"@unpic/react": "1.0.2",
		"downshift": "9.0.13",
		"es-toolkit": "1.44.0",
		"framer-motion": "12.24.7",
		"js-cookie": "3.0.5",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-icons": "5.5.0",
		"rehype-highlight": "7.0.2",
		"rehype-react": "8.0.0",
		"remark-emoji": "5.0.2",
		"remark-gfm": "4.0.1",
		"remark-github": "12.0.0",
		"remark-parse": "11.0.0",
		"remark-rehype": "11.1.2",
		"remark-stringify": "11.0.0",
		"semver": "7.7.3",
		"unified": "11.0.5"
	},
	"devDependencies": {
		"@chakra-ui/cli": "2.5.8",
		"@chakra-ui/styled-system": "2.12.0",
		"@chakra-ui/theme-tools": "2.2.6",
		"@chakra-ui/utils": "2.2.2",
		"@currents/playwright": "1.20.2",
		"@eslint-react/eslint-plugin": "2.7.2",
		"@eslint/compat": "2.0.1",
		"@eslint/js": "9.39.2",
		"@netlify/vite-plugin-tanstack-start": "1.2.7",
		"@octokit/openapi-types": "27.0.0",
		"@playwright/test": "1.57.0",
		"@sentry/vite-plugin": "4.6.2",
		"@stylistic/eslint-plugin": "5.7.0",
		"@tanstack/eslint-plugin-query": "5.91.3",
		"@tanstack/eslint-plugin-router": "1.154.7",
		"@tanstack/react-query-devtools": "5.91.2",
		"@types/js-cookie": "3.0.6",
		"@types/mdast": "4.0.4",
		"@types/node": "22.19.7",
		"@types/react": "19.2.9",
		"@types/react-dom": "19.2.3",
		"@types/semver": "7.7.1",
		"@types/unist": "3.0.3",
		"@vitejs/plugin-react": "5.1.2",
		"@vitest/browser-playwright": "4.0.17",
		"@vitest/coverage-v8": "4.0.17",
		"@vitest/eslint-plugin": "1.6.6",
		"@vitest/ui": "4.0.17",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import-x": "4.16.1",
		"eslint-plugin-jsx-a11y": "6.10.2",
		"eslint-plugin-playwright": "2.5.0",
		"eslint-plugin-react-hooks": "7.0.1",
		"eslint-plugin-unicorn": "62.0.0",
		"globals": "17.0.0",
		"happo": "6.2.4",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"msw": "2.12.7",
		"prettier": "3.8.1",
		"type-fest": "5.4.1",
		"typescript": "5.9.3",
		"typescript-eslint": "8.53.1",
		"vite": "7.3.1",
		"vite-tsconfig-paths": "6.0.4",
		"vitest": "4.0.17",
		"vitest-browser-react": "2.0.2"
	},
	"msw": {
		"workerDirectory": "public"
	}
}
